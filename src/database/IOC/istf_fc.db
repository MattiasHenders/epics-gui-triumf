#========================
# Faraday Cup Controller
#========================

record(bi, "$(IOC):SOL"){

   field(DESC, "Faraday Cup Compressed Air")
   field(INP, "0")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):IN"){

   field(DESC, "Faraday Cup IN")
   field(INP, "0")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):OUT"){

   field(DESC, "Faraday Cup OUT")
   field(INP, "1")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):HWR"){

   field(DESC, "Cooling Water is flowing")
   field(INP, "0")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):device"){
field(INP, "1")
field(DESC, "ISTF FC")
}
