#========================
# Faraday Cup Beam Sensor
#========================

record(ai, "$(IOC)"){

   field(DESC, "Analog Voltage Read of the Beam Current")
   field(INP, "0")
   field(SCAN, ".1 second")
   field(EGU, "V")
}

record(bi, "$(IOC):device"){
field(INP, "1")
field(DESC, "ISTF FC RDCUR")
}
