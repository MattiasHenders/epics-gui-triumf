#============================
# Gague Vacuum Controller
#============================

record(bi, "$(IOC):SOL"){

   field(DESC, "Controller of the Gague Vacuum - IN or OUT via Compressed Air")
   field(INP, "0")
   field(ZNAM, "LOW")
   field(ONAM, "HIGH")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):IN"){

   field(DESC, "Sensor for if the Gague Vacuum is IN all the way")
   field(INP, "0")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):OUT"){

   field(DESC, "Sensor for if the Gague Vacuum is OUT all the way")
   field(INP, "1")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):device"){
field(INP, "1")
field(DESC, "ISTF GV")
}
