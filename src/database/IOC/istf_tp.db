record(bi, "$(IOC):RP"){

   field(DESC, "Roughing Pump ON OFF")
   field(INP, "0")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(ZSV, "MAJOR")
   field(OSV, "NO_ALARM")
   field(SCAN, ".1 second")
}

