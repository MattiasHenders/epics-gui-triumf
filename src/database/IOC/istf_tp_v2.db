#=========================
# Turbo Pump V2 Controller
#=========================

record(bi, "$(IOC):RP"){

   field(DESC, "Roughing Pump ON or OFF")
   field(INP, "0")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):TP"){

   field(DESC, "Turbo Pump ON or OFF")
   field(INP, "0")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):VV1"){

   field(DESC, "Turbo Pump Isolation From Roughing Pump ON or OFF")
   field(INP, "1")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):VV2"){

   field(DESC, "Roughing Pump Isolation Valve ON or OFF")
   field(INP, "1")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):VV3"){

   field(DESC, "Roughing Valve For Entire System ON or OFF")
   field(INP, "1")
   field(ZNAM, "OFF")
   field(ONAM, "ON")
   field(SCAN, ".1 second")
}

record(bi, "$(IOC):device"){
field(INP, "1")
field(DESC, "ISTF TP")
}
