############################################################################
# FC 0
############################################################################
record(bi, "istf:fc0"){

field(DESC, "If the FC is OUT or IN")
field(INP, "0")
field(ZNAM, "OFF")
field(ONAM, "ON")
field(SCAN, ".1 second")
}


record(ai, "istf:fc0-voltage"){

field(SCAN, ".1 second")
field(HIGH, "11")
field(LOW, "0")
field(HSV, "NO_ALARM")
field(LSV, "MAJOR")
field(INP, "0")

}

