#========================
# Faraday Cup Controller
#========================

record(bi, "$(IOC):PWR"){

    field(DESC, "Demo LED Power")
    field(INP, "0")
    field(ZNAM, "OFF")
    field(ONAM, "ON")
    field(SCAN, ".1 second")
}

record(ai, "$(IOC):VOLTAGE"){

    field(DESC, "Faraday Cup IN")
    field(INP, "0")
    field(EGU, "V")
    field(SCAN, ".1 second")
}

record(bi, "$(IOC):SAFETY"){

    field(DESC, "Demo Safety Status")
    field(INP, "0")
    field(ZNAM, "OFF")
    field(ONAM, "ON")
    field(SCAN, ".1 second")
}